#include <stdlib.h>
#include <stdio.h>

/**
 * main - print product of multiplication of two arguments
 * @argc: counter of arguments
 * @argv: arguments
 * Return: (Success)
 */
int main(int argc, char **argv)
{
	/* first edge case Failure*/
	if (argc < 3)
	{
		printf("Error\n");
		return (EXIT_FAILURE);
	}

	/* normal case */

	printf("%d, %p\n", **argv, *argv + 1);

	return (EXIT_SUCCESS);
}
